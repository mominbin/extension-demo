{
  "name": "emotion-blog-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "json-server": "^1.0.0-alpha.21",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "react-scripts": "5.0.1",
    "swr": "^2.2.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "babel-plugin-react-intl": "^8.2.25",
    "babel-plugin-styled-components": "^2.1.4",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "plop": "^4.0.1",
    "prettier": "^3.1.1",
    "redux-saga": "^1.3.0",
    "shelljs": "^0.8.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint",
      "commit-msg": "yarn commitlint"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "prepare": "husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"' ",
    "server": "json-server --watch src/stub/blogApp.json --port 8000",
    "extract-intl": "node ./src/internals/scripts/extract-intl.js",
    "generate": "node src/internals/generators/component/index.mjs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
